<Window x:Class="StartupApplication.Shell"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:prsm="http://www.codeplex.com/prism"
        mc:Ignorable="d" ResizeMode="NoResize" WindowStartupLocation="CenterScreen"
        Title="BlueStacksAssignment"
        xmlns:cc="clr-namespace:CurrencyConverter;assembly=CurrencyConverter">
    <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
        <ContentControl prsm:RegionManager.RegionName="{x:Static cc:Constants.CURRENCY_CONVERTER_REGION }" />
    </StackPanel>
</Window>
